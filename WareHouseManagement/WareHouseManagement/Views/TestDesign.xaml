<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WareHouseManagement.Views.TestDesign">
    <TabbedPage.Children>
        <ContentPage Title="Profile" Icon="wareback13.png">
            <StackLayout Spacing="20" Padding="20" VerticalOptions="Center">
                <Entry Placeholder="Username" Text="vzv"/>
                <Entry Placeholder="Password" Text="vzzv" IsPassword="true"/>
                <Button Text="Login" TextColor="White" BackgroundColor="#77D065"/>
            </StackLayout>
        </ContentPage>
        <ContentPage Title="Settings" Icon="delivery_truck.png">
            <!-- Settings -->
        </ContentPage>
    </TabbedPage.Children>
</TabbedPage>
